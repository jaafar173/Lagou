*{
	padding:0px;
	margin:0px;
}
.logo_bac{
	background-color:#00B38B;
	width:100%;
	height:389;
	position:relative;
}
.logo_content{
	color:white;
	position:absolute;
	margin-top:30px;
	margin-left:50%;
}
.lunbo_ul img {
	width:150px;
	height:150px;
}
.lunbo_ul li{
	width:170px;
	height:220px;
	/*border:1px solid red;*/
	background:#CCF0E8;
	font-size:14px;
	float:left;
	margin-left:50px;
}
.lunbo_p{
	margin:10px 10px 10px 10px;

}

.lunbo_ul li p{
	margin-top:10px;
	text-align:center;
	z-index:1;
}
.lunbo_ul{
	position:absolute;
	margin-top:80px;
	margin-left:150px;

}

.lunbo_cont{
	width:190px;
	height:240px;
	background:#fff;
	opacity:0.9;
	filter:alpha(opacity=70);
	display:none;
	z-index:3;
	margin-top:-220px;
	margin-left:-10px;
	/*padding:10px;*/
}
