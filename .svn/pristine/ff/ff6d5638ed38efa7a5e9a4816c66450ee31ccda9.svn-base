*{
    padding: 0px;
    margin: 0px;
}
#lg_header{
    width: 100%;
    height: 86px;
    background-color:#fafafa; 
    border-bottom: 1px solid #e5e5e5;
}
#lg_tbar{
    width: 100%;
    height: 30px;
    background-color:black; 
}
.lg_tnav_wrap{
    float: left;
    width: 248px;
    height: 55px;
}
.lg_tnav_wrap img{
    float: left;
    margin-left: 100px;
    margin-top: 5px;
    display: inline;
}
#suggestCity{
    float: left;
    width: 260px;
    height: 54px;
}
#suggestCity strong{
    /*margin-right: 8px;*/
    margin-top: 20px;
    color: #333;
    font-size: 18px;
    font-weight: 400;
    line-height: 75px;
}
#suggestCity em{
    color: #999;
    font-size: 12px;
    font-style: normal;
    cursor:pointer;
    display: inline;
} 
em,i{
    font-style: normal;
}
.modal-header{
    background-color: #00B38A;
}
.tab_focus{
    width: 98px;
    height: 48px;
    line-height: 48px;
    border: 2px solid #ccc;
    padding: 15px;
}
..list_city{
    margin-top: 20px;
}
.list_city li{
    float: left;
    list-style: none;
    margin: 20px 0px 10px 60px;
}
.nav-pills li{
    float: left;
    list-style-type: none;
    margin: 8px 0px 0px 30px;
}
.nav-pills li a{
    font-size: 20px;
    color: #999;
    padding: 0 5px;

}