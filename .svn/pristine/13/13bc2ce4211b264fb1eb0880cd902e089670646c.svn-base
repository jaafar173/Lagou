<script type="text/javascript">
	//申请的js代码
	$(function(){
		$("#jihe_right").mouseover(function(){
			$(this).css({backgroundColor:"#00c195",color:"white"});
		}).mouseout(function(){
			$(this).css({backgroundColor:"#f2f2f2",color:"#00c195"});
		});
	});
	$(function(){
		$("#header_right a").mouseover(function(){
			$(this).css("color","white");
		}).mouseout(function(){
			$(this).css("color","#585858");
		});
		//轮播图效果
		$("#lunbo_left").mouseover(function(){
			$(this).css("opacity",0.7);
		}).mouseout(function(){
			$(this).css("opacity",0.3);
		})
		$("#lunbo_right").mouseover(function(){
			$(this).css("opacity",0.7);
		}).mouseout(function(){
			$(this).css("opacity",0.3);
		})
	});
	//轮播图的js
	$(function(){
		m=0;
		function show(m){
			for(var i=0;i<$(".lunbo_midum").length;i++){
				$(".lunbo_midum")[i].style.display=(i==m)?"block":"none";
			}
				}
		//点击的效果
		$("#lunbo_left").click(function(){
			clearTimeout(mytime);
			m=m-2;
			if(m<-1){
				m=2;
			}
			running();
		});
		$("#lunbo_right").click(function(){
			clearTimeout(mytime);
			m=m;
			running();
		});
		function running(){
			m++;
			if(m==$(".lunbo_midum").length){
				m=0;
			}
			show(m);
			mytime=setTimeout(running,2000);
		}
		running();
		});
	//解决冲突
$(function(){
	$("#logo_click1").css("borderBottom","3px solid #08cc7b");
	$("#logo_click2").mouseover(function(){
			$(this).css({borderBottom:"3px solid #08cc7b",color:"black"});
		}).mouseout(function(){
			$(this).css({borderBottom:""})
		})
});
</script>
